<h3>处理过的任务</h3>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
        <tr>
            <th>ID</th>
            <th>类型</th>
            <th>标题</th>
            <th>详情</th>
        </tr>
        </thead>
        <tbody>
            #foreach($task in $tasks)
            <tr>
                <td>$!{task.id}</td>
                <td>$!{task.type}</td>
                <td>$!{task.title}</td>
                <td><a href="$!{task.link}">审批</a></td>
            </tr>
            #end
        </tbody>
    </table>
</div>

