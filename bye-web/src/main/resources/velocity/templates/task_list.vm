<h3>待处理的任务</h3>
<div class="table-responsive">
    <table class="table table-striped table-sm">
        <thead>
        <tr>
            <th>ID</th>
            <th>标题</th>
            <th>审批</th>
        </tr>
        </thead>
        <tbody>
            #foreach($task in $tasks)
            <tr>
                <td>$!{task.processInstanceId}</td>
                <td>$!{task.title}</td>
                <td><a href="task_show.htm?processInstanceId=$!{task.processInstanceId}">审批</a></td>
            </tr>
            #end
        </tbody>
    </table>
</div>

